generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Workflow {
  id        String   @id @default(uuid())
  name      String   @default("Untitled Workflow")
  nodes     String   @default("[]") // SQLite doesn't support JSON type natively in Prisma
  edges     String   @default("[]") // SQLite doesn't support JSON type natively in Prisma
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
